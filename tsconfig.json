{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "*": [
        "src/*"
      ],
      "@root/*": [
        "*"
      ],
      "@src/*": [
        "src/*"
      ],
      "@apis/*": [
        "src/apis/*"
      ],
      "@assets/*": [
        "src/assets/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@core/*": [
        "src/core/*"
      ],
      "@hooks/*": [
        "src/hooks/*"
      ],
      "@i18n/*": [
        "src/i18n/*"
      ],
      "@navigation/*": [
        "src/navigation/*"
      ],
      "@screens/*": [
        "src/screens/*"
      ],
      "@services/*": [
        "src/services/*"
      ],
      "@store/*": [
        "src/store/*"
      ],
      "@utils/*": [
        "src/utils/*"
      ],
      "@theme/*": [
        "src/theme/*"
      ]
    }
  }
}